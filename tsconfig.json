{
  "compilerOptions": {
    "baseUrl": "null",
    "outDir": "./build",
    "allowJs": true,
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "esnext",
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2015.promise"
    ],
    "jsx": "react",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}